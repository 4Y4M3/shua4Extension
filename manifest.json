{
    "name": "shua 4 Extension",
    "description": "A lovely downloader",
    "version": "0.4",
    "author": "4Y4M3",
    "manifest_version": 3,

    "icons": {
        "128": "image/icon128.png"
    },

    "action": {
        "default_popup": "popup.html"
    },

    "background": {
      "service_worker": "background.js",
      "type": "module" 
    },

    "permissions": [ "tabs", "downloads" ],

    "homepage_url": "https://github.com/4Y4M3/shua4Extension"
}